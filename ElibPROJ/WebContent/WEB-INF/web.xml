<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>CRUD Project</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
  <servlet-name>LibController</servlet-name>
  <servlet-class>com.sanjay.controller.LibController</servlet-class>
  </servlet>
  <servlet>
  <servlet-name>BookController</servlet-name>
  <servlet-class>com.sanjay.controller.BookController</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>LibController</servlet-name>
  <url-pattern>/LibController.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  <servlet-name>BookController</servlet-name>
  <url-pattern>/BookController.do</url-pattern>
  </servlet-mapping>
</web-app>